{
	"name": "cep-any",
	"version": "3.2.1",
	"description": "A modern and low dependency cep consult lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/victorfernandesraton/cep-any"
	},
	"scripts": {
		"test": "jest",
		"lint": "npx eslint ./src",
		"build:source": "node ./script/build/build.cjs && node ./script/build/build.js",
		"build:types": "npx tsc",
		"build": "rm -rf ./dist && npm run build:types && npm run build:source",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promisse.any",
		"zip",
		"zipcode",
		"zipcode-brasil",
		"correios",
		"zip-br",
		"CEP",
		"cep-promise"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "5.38.1",
		"@typescript-eslint/parser": "5.38.1",
		"esbuild": "0.15.10",
		"esbuild-jest": "0.5.0",
		"eslint": "8.24.0",
		"jest": "29.1.1",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">= 17.0.0"
	},
	"exports": {
		".": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"types": "./dist/types.d.ts",
	"contributors": [
		{
			"name": "Victor Raton",
			"url": "https://github.com/victorfernandesraton"
		}
	],
	"files": [
		"./dist",
		"README.md"
	],
	"homepage": "https://github.com/victorfernandesraton/cep-any"
}
