{
	"name": "cep-any",
	"version": "3.1.0",
	"description": "A modern and low dependency cep consult lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/victorfernandesraton/cep-any"
	},
	"scripts": {
		"test:unit": "npm run test test/unit/*.ts",
		"test:e2e": "npm run test test/e2e/*.ts",
		"test": "NODE_OPTIONS=--experimental-vm-modules npx jest --maxWorkers=75%",
		"lint": "npx eslint ./src",
		"build:source": "node ./script/build/build.cjs && node ./script/build/build.js",
		"build:types": "npx tsc",
		"build": "rm -rf ./dist && npm run build:types && npm run build:source",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promisse.any",
		"zip",
		"zipcode",
		"zipcode-brasil",
		"correios",
		"zip-br",
		"CEP",
		"cep-promise"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"devDependencies": {
		"esbuild": "^0.15.5",
		"esbuild-jest": "0.5.0",
		"eslint": "^8.22.0",
		"jest": "28.1.3",
		"typescript": "4.7.4"
	},
	"engines": {
		"node": ">= 16.0.0"
	},
	"exports": {
		".": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"types": "./dist/types.d.ts",
	"contributors": [
		{
			"name": "Victor Raton",
			"url": "https://github.com/victorfernandesraton"
		}
	],
	"files": [
		"./dist",
		"README.md"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"homepage": "https://github.com/victorfernandesraton/cep-any"
}
