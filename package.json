{
	"name": "cep-any",
	"version": "1.1.0",
	"description": "A modern and low dependency cep consult lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/victorfernandesraton/cep-any"
	},
	"scripts": {
		"test:unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' npx mocha --parallel -r ts-node/register 'test/unit/*.ts'",
		"test:e2e": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' npx mocha  --parallel --timeout 100000 -r ts-node/register 'test/e2e/*.ts'",
		"test": "npm run test:unit && npm run test:e2e",
		"lint": "npx eslint ./src",
		"build": "npx rollup -c",
		"watch": "npx rollup -cw",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promise-any",
		"promisse.any",
		"zip",
		"zipcode",
		"zio-br"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"dependencies": {
		"axios": "0.26.0"
	},
	"devDependencies": {
		"@types/mocha": "^9.1.0",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"eslint": "8.10.0",
		"mocha": "^9.2.1",
		"rollup": "^2.69.0",
		"rollup-plugin-typescript2": "^0.31.2",
		"ts-jest": "27.1.3",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"engines": {
		"node": ">= 16.14.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist"
	],
	"contributors": [
		{
			"name": "Victor Raton",
			"url": "https://github.com/victorfernandesraton"
		}
	],
	"homepage": "https://github.com/victorfernandesraton/cep-any",
	"types": "dist/src/index.d.ts"
}
