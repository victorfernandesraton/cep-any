{
	"name": "cep-any",
	"version": "1.0.11",
	"description": "A modern and future ts/js dist for cep consult",
	"scripts": {
		"test:unit": "jest --verbose ./test/unit",
		"test:e2e": "jest --verbose ./test/e2e",
		"test:watch": "jest --verbose --watchAll",
		"test": "jest --verbose",
		"lint": "eslint ./src",
		"build:cjs": "rm -fr dist/cjs && tsc -p tsconfig.cjs.json",
		"build:esm": "rm -fr dist/esm && tsc -p tsconfig.esm.json",
		"build": "rollup -c",
		"watch": "rollup -cw",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promise-any",
		"zip",
		"zipcode",
		"zio-br"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"dependencies": {
		"axios": "0.26.0",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@types/jest": "27.4.1",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"eslint": "8.10.0",
		"jest": "27.5.1",
		"rollup": "^2.69.0",
		"rollup-plugin-typescript2": "^0.31.2",
		"ts-jest": "27.1.3",
		"typescript": "^4.5.5"
	},
	"engines": {
		"node": ">= 16.14.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist"
	],
	"types": "dist/src/index.d.ts"
}
