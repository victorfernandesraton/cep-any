{
	"name": "cep-any",
	"version": "1.2.2",
	"description": "A modern and low dependency cep consult lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/victorfernandesraton/cep-any"
	},
	"scripts": {
		"test:unit": "npm run test test/unit/*.ts",
		"test:e2e": "npm run test test/e2e/*.ts",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
		"lint": "npx eslint ./src",
		"build": "rm -r ./dist && npx swc ./src -o ./dist/index.js",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promisse.any",
		"zip",
		"zipcode",
		"zipcode-brasil",
		"correios",
		"zip-br",
		"CEP",
		"cep-promise"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"dependencies": {
		"axios": "0.26.0"
	},
	"devDependencies": {
		"@swc/cli": "0.1.57",
		"@swc/core": "1.2.165",
		"@swc/jest": "0.2.20",
		"@types/jest": "27.4.1",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"cross-env": "7.0.3",
		"eslint": "8.10.0",
		"jest": "27.5.1",
		"tslib": "2.3.1",
		"typescript": "4.6.3"
	},
	"engines": {
		"node": ">= 16.14.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"files": [
		"dist"
	],
	"contributors": [
		{
			"name": "Victor Raton",
			"url": "https://github.com/victorfernandesraton"
		}
	],
	"homepage": "https://github.com/victorfernandesraton/cep-any"
}
