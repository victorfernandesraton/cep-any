{
	"name": "cep-any",
	"version": "1.0.1",
	"description": "A modern and future ts/js lib for cep consult",
	"main": "./dist/cjs/index.js",
	"scripts": {
		"test:unit": "jest --verbose ./test/unit",
		"test:e2e": "jest --verbose ./test/e2e",
		"test:watch": "jest --verbose --watchAll",
		"test": "jest --verbose",
		"lint": "eslint ./src",
		"build:cjs": "rm -fr ./lin/cjs && tsc -p tsconfig.cjs.json",
		"build:esm": "rm -fr ./lin/esm && tsc -p tsconfig.esm.json",
		"build": "npm run build:esm && npm run build:cjs"
	},
	"keywords": [],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"dependencies": {
		"axios": "0.26.0"
	},
	"devDependencies": {
		"@types/jest": "27.4.1",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"eslint": "8.10.0",
		"jest": "27.5.1",
		"ts-jest": "27.1.3",
		"typescript": "4.5.5"
	},
	"module": "./dist/esm/index.js",
	"engines": {
		"node": ">= 16.14.0"
	},
	"directories": {
		"lib": "./lib"
	},
	"typings": "./lib/types/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/esm/index.js",
			"require": "./lib/cjs/index.js",
			"default": "./lib/esm/index.js"
		}
	}
}
