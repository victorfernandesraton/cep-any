{
	"name": "cep-any",
	"version": "4.0.0",
	"description": "A modern and zero dependency cep consult lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/victorfernandesraton/cep-any"
	},
	"scripts": {
		"test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
		"lint": "npx eslint ./src",
		"build:source": "node esbuild.config.js",
		"build:types": "npx tsc",
		"build": "rm -rf ./dist && npm run build:types && npm run build:source",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cep",
		"cep-any",
		"promisse.any",
		"zip",
		"zipcode",
		"zipcode-brasil",
		"correios",
		"zip-br",
		"CEP",
		"cep-promise"
	],
	"author": {
		"name": "Victor Fernandes Raton",
		"email": "vfbraton@gmail.com",
		"url": "https://github.com/victorfernandesraton"
	},
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"esbuild": "0.19.12",
		"esbuild-jest": "0.5.0",
		"eslint": "8.45.0",
		"jest": "^29.7.0",
		"typescript": "5.3.3"
	},
	"engines": {
		"node": ">= 18"
	},
	"exports": {
		".": {
			"import": "./dist/esm.mjs",
			"require": "./dist/index.js"
		}
	},
	"types": "./dist/types.d.ts",
	"contributors": [
		{
			"name": "Victor Raton",
			"url": "https://github.com/victorfernandesraton"
		}
	],
	"bin": {
		"cep-any": "/bin/cep-any.js"
	},
	"files": [
		"bin",
		"dist",
		"README.md",
		"LICENSE"
	],
	"homepage": "https://github.com/victorfernandesraton/cep-any"
}
